		<OnPlatform x:TypeArguments="Thickness" iOS="0, 40, 0, 0" />
	</ContentPage.Padding>
	<ContentPage.Content>
		<AbsoluteLayout BackgroundColor="White" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
			<Grid AbsoluteLayout.LayoutBounds="0.5, 1, 1, 0.7" AbsoluteLayout.LayoutFlags="All">
				<TableView x:Name="tblRequests">
					<TableView.Root>
						<TableSection Title=" " x:Name="mainTableSection">
						</TableSection>
					</TableView.Root>
				</TableView>
			</Grid>
		</AbsoluteLayout>
	</ContentPage.Content>
</ContentPage>
